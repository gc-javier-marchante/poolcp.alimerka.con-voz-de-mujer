<div class="scroll-anchor" id="home"></div>
<section class="bg-cover bg-image-navidad bg-top bg-light-grey d-flex align-items-top mb-4">
	<div class="container">
		<div class="row g-0">
			<div class="col-12 order-2 col-lg-6 order-lg-1 align-self-end">
				<img src="{{ root }}static/img/chefs-silu.png" class="w-100" alt="Pepe RodrÃ­guez y Nacho Manzano">
			</div>
			<div class="col-12 order-1 col-lg-6 order-lg-2 text-end">
				<div class="jumbotron bg-transparent mb-0 py-0">
					{% if website_is_closed %}
					{% include 'elements/header-finish.twig' %}
					{% else %}
					{% if used %}
					{% include 'elements/header-used.twig' %}
					{% elseif invalid %}
					{% include 'elements/header-invalid.twig' %}
					{% elseif generic_error %}
					{% include 'elements/header-generic-error.twig' %}
					{% else %}
					{% include 'elements/header-pincode.twig' %}
					{% endif %}
					{% endif %}
					{#
					{% include 'elements/header-winner.twig' %}
					{% include 'elements/header-loser.twig' %}
					#}
				</div>
			</div>
		</div>
	</div>
</section>

